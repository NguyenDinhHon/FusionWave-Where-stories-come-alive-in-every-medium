{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FusionWave (web-server)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "web-server",
                "--web-hostname",
                "localhost",
                "--web-port",
                "8080"
            ]
        },
        {
            "name": "FusionWave (Edge)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--web-port",
                "8080"
            ]
        }
    ]
}
